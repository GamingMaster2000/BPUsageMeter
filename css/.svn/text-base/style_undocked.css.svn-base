body
{
	margin:0;
	padding:0;
	width: 200px;
	height: 185px;
	font-family: Arial, Verdana, "SF Sans Serif";
	font-weight: normal;
	color: #000000;
	background-color: #FFFFFF;
	border: solid 1px #000000
}

#container
{
	width: 198px;
	padding: 0px 8px 0px 8px;;
}

#barEmpty1, #barEmpty2
{
	width:184px; 
	height:28px; 
	background-color: #CCCCCC;	
	margin-bottom: 1px;	
}

#bar1
{
	background-color: #FF6600;
}

#bar2
{
	background-color: #3399CC;
}

.barFill
{
	width:1px; 
	height:28px; 
	float:left;
}

#mainheader
{
	color:#000;
	font-family: Calibri;
	font-size:26px;
	font-weight: bold;
	padding: 2px 6px 0px 6px;
	line-height: 100%;
}

#usageheader
{
	position: absolute;
	left: 13px;
	top: 37px;
	font-family: Calibri;
	font-size:20px;
	color:#FFF;
	font-weight: bold;

}

#monthheader
{
	position: absolute;
	left: 13px;
	top: 99px;
	font-family: Calibri;
	font-size:20px;
	color:#FFF;
	font-weight: bold;
}

#usageblock
{
	padding-top: 5px;	
}

#monthblock, #controlblock
{
	margin-top: 4px;	
	padding-top: 3px;
}

#usageBlock, #monthBlock, #controlBlock
{ 
	clear:both;
	border-top: dotted 1px #000000;
}

.font
{
	font-size: 18px;
}

.font10
{
	font-size: 16px;
}


.icon 
{
	float: left;
	position: relative;
	width:19px;
	height:19px;
	margin-right: 4px;
	cursor: pointer;
}

.icon-details 
{
	border: #000 1px solid;
	width: 17px;
	height: 17px;
	position: absolute;
	top: 0;
	left: 0px;
}

.icon-details-box
{
	background-color: #000;
	width: 9px;
	height: 9px;
	position: absolute;
	top: 0px;
	left: 0px;
}

.icon-refresh
{
	border: #000 1px solid;
	width: 17px;
	height: 17px;
	position: absolute;
	top:0;
	left:0;
}

.icon-refresh-triangle 
{
	border-left:7px solid transparent;
  	border-right:7px solid transparent;
  	border-top:7px solid #000;
	height: 0;
	width: 0;
	position: absolute;
	bottom: 2px;
	left: 1px;
}

.icon-refresh-line 
{
	background-color: #000;
	width: 5px;
	height: 11px;
	position: absolute;
	top:0px;
	left:6px;
}

#accountName{
	text-align: center;	
}